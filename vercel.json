{
  "version": 2,
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "builds": [
    {
      "src": "navishop/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "navishop/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/navishop/index.html"
    }
  ]
}
