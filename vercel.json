{
  "version": 2,
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "builds": [
    {
      "src": "navishop/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    },
    {
      "src": "navishop/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/navishop/static/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/navishop/images/$1"
    },
    {
      "src": "/(.*\\.(js|css|map|ico|png|jpg|jpeg|gif|svg|webp|json|txt|woff|woff2|gltf))$",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/navishop/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/navishop/index.html"
    }
  ]
}
