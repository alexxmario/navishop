{
  "version": 2,
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|map|ico|png|jpg|jpeg|gif|svg|webp))$",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/build/$1"
    },
    {
      "src": "/logos/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/build/logos/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ]
}